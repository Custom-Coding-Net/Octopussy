<list name="AAT_Mime">

	<item label="Image Bitmap (BMP)" value="bmp"
        regexp="image\/bmp" logo="bmp" />
	<item label="Archive BZIP2 (bz2)" value="bz2"
        regexp="x-bzip2" logo="bz2" />
	<item label="Calendar" value="calendar"
        regexp="text\/calendar" logo="vcalendar" />
	<item label="Cascading Style Sheet (CSS)" value="css"
        regexp="text\/css" logo="css" />
	<item label="Debian Package (DEB)" value="deb"
        regexp="debian-package" logo="deb" />
	<item label="Flash" value="flash"
				regexp="x-shockwave-flash" logo="flash" />
	<item label="Video Flash" value="flv"
        regexp="video\/(x-)?flv" logo="video" />
	<item label="Image GIF (GIF)" value="gif"
        regexp="image\/gif" logo="gif" />
	<item label="Archive GZIP (gz)" value="gzip"
        regexp="application\/(x-)*gzip" logo="gz" />
	<item label="HyperText Markup Language (HTML)" value="html"
        regexp="text\/html" logo="html" />
	<item label="Applet Java" value="java"
        regexp="x-java-applet" logo="js" />
	<item label="Archive Java" value="java"
        regexp="java-archive" logo="java" />
	<item label="Image JPEG (JPG)" value="jpg"
        regexp="image\/jpe?g" logo="jpg" />
	<item label="Javascript (JS)" value="js"
        regexp="javascript" logo="js" />
	<item label="Portable Document Format (PDF)" value="pdf"
        regexp="application\/pdf" logo="pdf" />
	<item label="Image PNG (PNG)" value="png"
        regexp="image\/png" logo="png" />
	<item label="Really Simple Syndication (RSS)" value="rss"
        regexp="application\/rss\+xml" logo="rss" />
	<item label="Resource Description Framework (RDF)" value="rdf"
        regexp="application\/rdf\+xml" logo="rss" />
	<item label="Resource Description Framework (RDF)" value="rdf"
        regexp="text\/rdf" logo="rss" />
	<item label="Stream" value="stream"
        regexp="application\/octet-stream" logo="stream" />
	<item label="Archive TAR (tar)" value="tar"
        regexp="x-tar" logo="tar" />
	<item label="Plain text (TXT)" value="txt"
        regexp="text\/plain" logo="txt" />
	<item label="eXtensible HyperText Markup Language (XHTML)" value="xhtml"
        regexp="xhtml" logo="xhtml" />
	<item label="eXtensible Markup Language (XML)" value="xml"
        regexp="\w+\/xml" logo="xml" />

</list>
