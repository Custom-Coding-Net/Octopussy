<octopussy_service name="Nagios_NSCA"
                   description="Nagios NSCA Service"
                   icon="software/logo_nagios"
                   website="http://www.nagios.org"
                   version="200703080001">
  <message loglevel="Notice"
           msg_id="Nagios nsca:connection_from"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Connection from .+ port \d+&quot;):msg@&gt;"
           rank="002"
           table="Message"
           taxonomy="Access" />
  <message loglevel="Information"
           msg_id="Nagios nsca:host_address_ok"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Host address checks out ok&quot;):msg@&gt;"
           rank="004"
           table="Message"
           taxonomy="System" />
  <message loglevel="Information"
           msg_id="Nagios nsca:handling_the_connection"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Handling the connection...&quot;):msg@&gt; "
           rank="005"
           table="Message"
           taxonomy="System" />
  <message loglevel="Information"
           msg_id="Nagios nsca:end_of_connection"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;End of connection...&quot;):msg@&gt; "
           rank="006"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Nagios nsca:service_check"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: SERVICE CHECK -&gt; Host Name: &lt;@SINGLE_QUOTED_STRING:host@&gt;, Service Description: &lt;@SINGLE_QUOTED_STRING:service@&gt;, Return Code: &lt;@SINGLE_QUOTED_STRING:status@&gt;, Output: &lt;@SINGLE_QUOTED_STRING:msg@&gt; "
           rank="001"
           table="Nagios_service"
           taxonomy="System" />
  <message loglevel="Warning"
           msg_id="Nagios nsca:not_allowed_talk"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Host \S+ is not allowed to talk to us!&quot;):msg@&gt; "
           rank="007"
           table="Message"
           taxonomy="Access.Failure" />
  <message loglevel="Warning"
           msg_id="Nagios nsca:command_file_doesnot_exist"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Command file .+ does not exist, attempting to use alternate dump file .+ for output&quot;):msg@&gt;"
           rank="008"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Information"
           msg_id="Nagios_NSCA:connect_from"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;connect from .+&quot;):msg@&gt;"
           rank="003"
           table="Message"
           taxonomy="Network" />
  <message loglevel="Notice"
           msg_id="Nagios_NSCA:starting_up_daemon"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Starting up daemon&quot;):msg@&gt; "
           rank="009"
           table="Message"
           taxonomy="System" />
  <message loglevel="Warning"
           msg_id="Nagios_NSCA:dropping_packet_stale_timestamp"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Dropping packet with stale timestamp - packet was \d+ seconds old.&quot;):msg@&gt; "
           rank="010"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Warning"
           msg_id="Nagios_NSCA:network_server_bind_failure"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Network server bind failure.+&quot;):msg@&gt;"
           rank="011"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Critical"
           msg_id="Nagios_NSCA:unknown_option_in_file"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; nsca[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;Unknown option specified in config file .+&quot;):msg@&gt;"
           rank="012"
           table="Message"
           taxonomy="Config.Errors" />
</octopussy_service>
