<octopussy_service name="Linux_PAM"
                   description="Linux PAM Service"
									 icon="operating_systems/os_linux_generic"
                   version="200610050001">
  <message loglevel="Warning"
           msg_id="Linux_PAM:user_access_denied"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_access[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;access denied for user .+&quot;):msg@&gt;"
           rank="01"
           table="Linux_PAM_msg"
           taxonomy="Access.Failure" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:no_day_specified"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_group[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;no day specified&quot;):msg@&gt; "
           rank="02"
           table="Linux_PAM_msg"
           taxonomy="Config.Errors" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:bad_field_count"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_access[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;.+: line \d+: bad field count&quot;):msg@&gt; "
           rank="03"
           table="Linux_PAM_msg"
           taxonomy="Config.Errors" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:password_too_short"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_pwdfile[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;password too short or NULL&quot;):msg@&gt; "
           rank="04"
           table="Linux_PAM_msg"
           taxonomy="Auth.Failure" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:wrong_password"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_pwdfile[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;wrong password for user .+&quot;):msg@&gt;"
           rank="05"
           table="Linux_PAM_msg"
           taxonomy="Auth.Failure" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:console_getpwnam_failed"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; pam_console[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;getpwnam failed for .+&quot;):msg@&gt;"
           rank="06"
           table="Linux_PAM_msg"
           taxonomy="Auth.Failure" />
  <message loglevel="Warning"
           msg_id="Linux_PAM:unable_create_directory"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; PAM-mkhomedir[&lt;@NUMBER:pid@&gt;]: &lt;@REGEXP(&quot;unable to create directory .+&quot;):msg@&gt;"
           rank="07"
           table="Linux_PAM_msg"
           taxonomy="System.Errors" />
</octopussy_service>
