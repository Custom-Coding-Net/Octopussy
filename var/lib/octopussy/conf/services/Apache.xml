<octopussy_service name="Apache"
                   description="Apache Service"
									 icon="software/logo_apache"
                   version="200704300001">
  <message loglevel="Critical"
           msg_id="Apache:syntax_error_on_line"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;Syntax error on line \d+ of .+:&quot;):msg@&gt;"
           rank="001"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Critical"
           msg_id="Apache:invalid_command"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;Invalid command .+, perhaps mis-spelled or defined by a module not included in the server configuration&quot;):msg@&gt;"
           rank="002"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Critical"
           msg_id="Apache:cannot_load_module"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;Cannot load .+ into server: .*&quot;):msg@&gt;"
           rank="003"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Warning"
           msg_id="Apache:startup_failed"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;\S+ startup failed&quot;):msg@&gt; "
           rank="004"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Warning"
           msg_id="Apache:shutdown_failed"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;\S+ shutdown failed&quot;):msg@&gt; "
           rank="005"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Notice"
           msg_id="Apache:hup_succeeded"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;\S+ -HUP succeeded&quot;):msg@&gt;"
           rank="006"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Apache:unable_include_file"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;[error] .+ unable to include .+ in parsed file .+&quot;):msg@&gt;"
           rank="007"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Notice"
           msg_id="Apache:file_does_not_exist"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;[error] .+ File does not exist: .+&quot;):msg@&gt; "
           rank="008"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Notice"
           msg_id="Apache:various_errors"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;[error] .+&quot;):msg@&gt;"
           rank="010"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Warning"
           msg_id="Apache:authentication_failure"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@WORD:server@&gt;: &lt;@REGEXP(&quot;[error] .+ Authentication failure.+&quot;):msg@&gt;"
           rank="009"
           table="Message"
           taxonomy="Auth.Failure" />
  <message loglevel="Warning"
           msg_id="Apache:pam_ldap_error_bind"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; httpd[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;pam_ldap: error trying to bind as user .+&quot;):msg@&gt;"
           rank="011"
           table="Message"
           taxonomy="Auth.Failure" />
  <message loglevel="Warning"
           msg_id="Apache:certificate_doesnt_match_servername"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; httpd[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[warn] RSA server certificate CommonName .+ does NOT match server name.+&quot;):msg@&gt;"
           rank="012"
           table="Message"
           taxonomy="System.Errors" />
</octopussy_service>
