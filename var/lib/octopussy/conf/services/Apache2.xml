<?xml version='1.0' encoding='iso-8859-1'?>
<octopussy_service name="Apache2"
                   description="Apache2 Service"
                   icon="software/logo_apache"
									 website="http://httpd.apache.org/"
                   version="200701290001">
  <message loglevel="Notice"
           msg_id="Apache2:php_warning"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@REGEXP(&quot;apache2\S*: PHP Warning: .+&quot;):msg@&gt;"
           rank="005"
           table="Message"
           taxonomy="System" />
  <message loglevel="Critical"
           msg_id="Apache2:php_fatal_error"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@REGEXP(&quot;apache2\S*: PHP Fatal error: .+&quot;):msg@&gt;"
           rank="006"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Critical"
           msg_id="Apache2:php_parse_error"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@REGEXP(&quot;apache2\S*: PHP Parse error:.+&quot;):msg@&gt;"
           rank="007"
           table="Message"
           taxonomy="System.Errors" />
  <message loglevel="Notice"
           msg_id="Apache2:file_does_not_exist"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[error] .+ File does not exist: .+&quot;):msg@&gt;"
           rank="001"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Apache2:script_not_found"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;.+ script .+ not found or unable to stat.+&quot;):msg@&gt; "
           rank="002"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Apache2:directory_index_forbidden"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[error] .+ Directory index forbidden by rule: .+&quot;):msg@&gt;"
           rank="003"
           table="Message"
           taxonomy="Access.Failure" />
  <message loglevel="Notice"
           msg_id="Apache2:unable_include_file"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[error] .+ unable to include .+ in parsed file .+&quot;):msg@&gt;"
           rank="004"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Notice"
           msg_id="Apache2:php_notice"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@REGEXP(&quot;apache2\S*: PHP Notice: .+&quot;):msg@&gt;"
           rank="008"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Information"
           msg_id="Apache2:pajax_any_message"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;PAJAX: .+&quot;):msg@&gt;"
           rank="009"
           table="Message"
           taxonomy="System" />
  <message loglevel="Warning"
           msg_id="Apache2:couldnt_connect_ldap_server"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; &lt;@REGEXP(&quot;apache\S+&quot;):NULL@&gt;: &lt;@REGEXP(&quot;nss_ldap: could not connect to any LDAP server.+&quot;):msg@&gt;"
           rank="010"
           table="Message"
           taxonomy="Network.Errors" />
  <message loglevel="Warning"
           msg_id="Apache2:htaccess_invalid_command"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[alert] [client \S+] .+htaccess: Invalid command .+&quot;):msg@&gt;"
           rank="011"
           table="Message"
           taxonomy="Config.Errors" />
  <message loglevel="Warning"
           msg_id="Apache2:denied_by_server_configuration"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2[&lt;@NUMBER:NULL@&gt;]: &lt;@REGEXP(&quot;[error] [client .+] client denied by server configuration: .+&quot;):msg@&gt;"
           rank="012"
           table="Message"
           taxonomy="Access.Failure" />
  <message loglevel="Information"
           msg_id="Apache2:pajax_any_message2"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; apache2: &lt;@REGEXP(&quot;PAJAX: .+&quot;):msg@&gt;"
           rank="013"
           table="Message"
           taxonomy="System" />
</octopussy_service>
