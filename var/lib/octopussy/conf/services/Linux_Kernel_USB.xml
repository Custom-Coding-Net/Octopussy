<?xml version='1.0' encoding='UTF-8'?>
<octopussy_service description="Linux Kernel USB Service"
                   icon="operating_systems/os_linux_generic"
                   name="Linux_Kernel_USB"
                   version="200901290001"
                   website="">
  <message loglevel="Debug"
           msg_id="Linux_Kernel_USB:storage_info"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.*usb-storage: .+&quot;):msg@&gt;"
           rank="001"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Linux_Kernel_USB:support_registered"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.*USB Mass Storage support registered.*&quot;):msg@&gt;"
           rank="002"
           table="Message"
           taxonomy="System" />
  <message loglevel="Notice"
           msg_id="Linux_Kernel_USB:new_device_using"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.+: new .+ USB device using .+&quot;):msg@&gt;"
           rank="003"
           table="Message"
           taxonomy="Hardware" />
  <message loglevel="Warning"
           msg_id="Linux_Kernel_USB:maybe_usb_cable_bad"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.+: Cannot enable port \d+.  Maybe the USB cable is bad.+&quot;):msg@&gt; "
           rank="004"
           table="Message"
           taxonomy="Hardware" />
	<message loglevel="Notice"
           msg_id="Linux_Kernel_USB:disconnect"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.+: USB disconnect, address \d+&quot;):msg@&gt; "
           rank="005"
           table="Message"
           taxonomy="Hardware" />	
  <message loglevel="Debug"
           msg_id="Linux_Kernel_USB:configuration_chosen"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.*usb .+: configuration #\d+ chosen from \d+ choice.*&quot;):msg@&gt;"
           rank="006"
           table="Message"
           taxonomy="Hardware" />
  <message loglevel="Debug"
           msg_id="Linux_Kernel_USB:usbcore_registered_new_driver"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.*usbcore: registered new driver .+&quot;):msg@&gt;"
           rank="007"
           table="Message"
           taxonomy="System.Boot" />
  <message loglevel="Debug"
           msg_id="Linux_Kernel_USB:scsi_emulation_mass_storage"
           pattern="&lt;@DATE_TIME_SYSLOG:datetime@&gt; &lt;@WORD:device@&gt; kernel: &lt;@REGEXP(&quot;.*scsi\d+ : SCSI emulation for USB Mass Storage devices&quot;):msg@&gt;"
           rank="008"
           table="Message"
           taxonomy="System.Boot" />
</octopussy_service>
